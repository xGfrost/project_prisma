generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Blogs{
  id          String @id @default(cuid())
  name        String
  description String
  image       String
  status      Boolean @default(true)
  type        String
  video_url   String
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
